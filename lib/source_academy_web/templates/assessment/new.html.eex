<%= admin_header @conn, "Create Assessment" %>

<div class="sa-admin-content">
  <%= if @changeset.action do %>
    <div class="pt-callout pt-intent-danger">
      Please fix the following problem
    </div>
  <% end %>
  <%= render "form.html",
        changeset: @changeset,
        method: "post",
        action: assessment_path(@conn, :create) %>
</div>
